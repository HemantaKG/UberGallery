{
    "name": "phlak/ubergallery",
    "description": "The simple PHP web photo gallery",
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Kankiewicz",
            "email": "Chris@ChrisKankiewicz.com"
        }
    ],
    "support": {
        "email": "UberGallery@GoogleGroups.com",
        "issues": "https://github.com/PHLAK/UberGallery/issues"
    },
    "require": {
        "php": ">=7.1.3",
        "ext-imagick": "*",
        "mustache/mustache": "^2.12",
        "phlak/config": "^4.0",
        "phlak/stash": "^6.0",
        "slim/slim": "^3.9",
        "tightenco/collect": "^5.6",
        "vlucas/phpdotenv": "^2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0 || ^7.0",
        "symfony/css-selector": "^4.0",
        "symfony/dom-crawler": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": ["app/Support/helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
